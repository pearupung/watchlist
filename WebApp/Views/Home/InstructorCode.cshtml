@model WebApp.ViewModels.HomeInstructorDTO

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<h2>title</h2>
<form id="account" method="post">
            <h4>Enter the code given by a pupil:</h4>
            <hr/>
            <div asp-validation-summary="All" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="@Model.EntryCode"></label>
                <input asp-for="@Model.EntryCode" class="form-control" />
                <span asp-validation-for="@Model.EntryCode" class="text-danger"></span>
            </div>
            <div class="form-group">
                <button  type="submit" class="btn btn-primary">Begin session</button>
            </div>
        </form>
